{
  "include": ["env.d.ts", "components.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "outDir": "es",
    "target": "ES2015",
    "module": "ES2020",
    "moduleResolution": "node",
    "removeComments": true,
    "strict": true,
    "allowJs": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "preserve"
  }
}
